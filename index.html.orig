    <script>
var container, camera, scene, renderer, object;

	var windowHalfX = window.innerWidth / 2;
	var windowHalfY = window.innerHeight / 2;

function isUserMobile()
{
	return /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent);
}

function animate() {

	requestAnimationFrame( animate );
	render();

}

function render() {

	if (object) {
		switch (object.def.rotateOn) {
		case "X":
			object.rotation.x += object.def.rotateBy;
			break;
		case "Y":
			object.rotation.y += object.def.rotateBy;
			break;
		case "Z":
			object.rotation.z += object.def.rotateBy;
			break;
		}; 
	}
		
	
	
	camera.lookAt( new THREE.Vector3(0, 0, 0));
	renderer.render( scene, camera );

}

var models = [
	
	{
		obj : "/obj/juno/juno.obj",
		mtl : "/obj/juno/juno.mtl",
		position : [17, 7, 0],
		rotation : [0, 15 * (Math.PI / 180), 90 * (Math.PI / 180), 'YXZ'],
		rotateOn : 'X',
		rotateBy : 0.2  * (Math.PI / 180),
		scale : 1.0
	},
	{
		obj : "/obj/voyager/voyager.obj",
		mtl : "/obj/voyager/voyager.mtl",
		position : [17, 7, 0],
		rotation : [0, 140 * (Math.PI / 180), 0 * (Math.PI / 180), 'YXZ'],
		rotateOn : 'Z',
		rotateBy : 0.1 * (Math.PI / 180),
		scale : 7.0
	},
	{
		obj : "/obj/cassini/cassini.obj",
		mtl : "/obj/cassini/cassini.mtl",
		position : [17, 7, 0],
		rotation : [90 * (Math.PI / 180), 140 * (Math.PI / 180), -45 * (Math.PI / 180), 'XYZ'],
		rotateOn : 'X',
		rotateBy : 0.1 * (Math.PI / 180),
		scale : 1.5
	},
	{
		obj : "/obj/iss/iss.obj",
		mtl : "/obj/iss/iss.mtl",
		position : [15, 5, 0],
		rotation : [0 * (Math.PI / 180), 0 * (Math.PI / 180), 0 * (Math.PI / 180), 'XYZ'],
		rotateOn : 'Y',
		rotateBy : 0.2 * (Math.PI / 180),
		scale : 4.0
	},
	{
		obj : "/obj/pioneer/pioneer.obj",
		mtl : "/obj/pioneer/pioneer.mtl",
		position : [17, 8, 0],
		rotation : [0 * (Math.PI / 180), 30 * (Math.PI / 180), 0 * (Math.PI / 180), 'XYZ'],
		rotateOn : 'Z',
		rotateBy : 0.1 * (Math.PI / 180),
		scale : 3.0
	},
	{
		obj : "/obj/kepler/kepler.obj",
		mtl : "/obj/kepler/kepler.mtl",
		position : [17, 8, 0],
		rotation : [0 * (Math.PI / 180), 30 * (Math.PI / 180), 45 * (Math.PI / 180), 'XYZ'],
		rotateOn : 'X',
		rotateBy : 0.1 * (Math.PI / 180),
		scale : 3.0
	},
	{
		obj : "/obj/rosetta/rosetta.obj",
		mtl : "/obj/rosetta/rosetta.mtl",
		position : [17, 8, 0],
		rotation : [20 * (Math.PI / 180), 0 * (Math.PI / 180), -30 * (Math.PI / 180), 'XYZ'],
		rotateOn : 'Y',
		rotateBy : 0.1 * (Math.PI / 180),
		scale : 1.0
	}
	
];


function init(modelDef) {
	var width = 1170;
	var height = 700;
	container = document.getElementById( 'jumbotron' )
	
	camera = new THREE.PerspectiveCamera( 45, width / height, 1, 2000 );
	camera.position.z = 40;

	scene = new THREE.Scene();

	var ambient = new THREE.AmbientLight( 0x303030 );
	scene.add( ambient );

	var directionalLight = new THREE.DirectionalLight( 0xFFFFFF );
	directionalLight.position.set( -10, 3, 10 ).normalize();
	scene.add( directionalLight );

	var loader = new THREE.OBJMTLLoader();
	loader.load( modelDef.obj, modelDef.mtl, function ( model ) {
		object = model;
		object.scale.set(modelDef.scale, modelDef.scale, modelDef.scale);
		object.def = modelDef;
		object.position.y = modelDef.position[1];
		object.position.x = modelDef.position[0];
		object.position.z = modelDef.position[2];
		object.rotation.set(object.def.rotation[0], object.def.rotation[1], object.def.rotation[2], object.def.rotation[3]);
		object.traverse(function(obj) {
			if (obj.material) {
				obj.material.shading = THREE.SmoothShading;
			}
		});
			
		scene.add( object );

	} );

	//

	renderer = new THREE.WebGLRenderer({antialias:true});
	renderer.setSize( width,  height );
	container.appendChild( renderer.domElement );
	$(renderer.domElement).css("z-index", "1000");
}
	
$(function() {
	
	
	$(".btn").on({
		mouseenter : function(event) {
			$(this).addClass("animated pulse");
		},
		mouseleave : function(event) {
			$(this).removeClass("animated pulse");
		}
	});
	
	$(".thumbnail").on({
		mouseenter : function(event) {
			$(this).addClass("animated pulse");
		},
		mouseleave : function(event) {
			$(this).removeClass("animated pulse");
		}
	});
	
	
	//$(".jumbotron").css("display", "block").addClass("animated bounceIn");
	
	
	
	var currentActivePill = "visualizations";
	
	$("#pill-visualizations").click(function(e) {
		$("#pill-"+currentActivePill).removeClass("active");
		$("#pill-visualizations").addClass("active");
		
		$("#"+currentActivePill).css("display", "none");
		$("#visualizations").css("display", "block");
		currentActivePill = "visualizations";
	});
	
	$("#pill-animations").click(function(e) {
		$("#pill-"+currentActivePill).removeClass("active");
		$("#pill-animations").addClass("active");
		$("#"+currentActivePill).css("display", "none");
		$("#animations").css("display", "block");
		currentActivePill = "animations";
	});
	
	$("#pill-images").click(function(e) {
		$("#pill-"+currentActivePill).removeClass("active");
		$("#pill-images").addClass("active");
		$("#"+currentActivePill).css("display", "none");
		$("#images").css("display", "block");
		currentActivePill = "images";
	});
	
	

	if (!isUserMobile() && (window.WebGLRenderingContext || Detector.webgl)) {
		$(".jumbotron").addClass("jumbotron-animated");
		init(models[Math.floor((Math.random()*models.length))]);
		animate();
	} else {
		$(".jumbotron").addClass("jumbotron-static");
		
	}
	

	
	
	
});	
	
	</script>
